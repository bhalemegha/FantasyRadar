<section class="players">
<section class="latest-news">
  <header>Recent News</header>
    <% @latest_notes.each do |note| %>
      <ul>
        <strong><%= note.note_date %></strong>
        <%= note.note_preview %>
        
      </ul>
    <% end %>
</section>
  <table>
    <thead>
      <tr>
        <th>Team</th>
        <th>Player Name</th>
        <th>Most Recent Note</th>
        <th>Select Player</th>
      </tr>
    </thead>
    <tbody>
        <% @players.each do |player| %>
          
          <%= render player %>
        <% end %>   
    </tbody>
  </table>
</section>
